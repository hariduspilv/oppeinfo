<jxb:bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:kutse="http://producers.kutseregister.xtee.riik.ee/producer/kutseregister"
              jxb:version="2.1">
  <jxb:globalBindings>
    <jxb:javaType name="java.time.LocalDate" xmlType="xsd:date"
        parseMethod="ee.hois.soap.SoapUtil.parseDate"
        printMethod="ee.hois.soap.SoapUtil.printDate"/>
    <jxb:javaType name="java.lang.String" xmlType="kutse:taseType"
        parseMethod="ee.hois.soap.SoapUtil.parseTaseType"
        printMethod="ee.hois.soap.SoapUtil.printTaseType"/>
  </jxb:globalBindings>
  <jxb:bindings schemaLocation="kutseregister.wsdl#types1" node="//xsd:element[@name='muutunudKutsestandardidParing']">
    <jxb:bindings node="//xsd:element[@name='alates']">
      <jxb:javaType name="java.time.LocalDate"
          parseMethod="ee.hois.soap.SoapUtil.parseDate"
          printMethod="ee.hois.soap.SoapUtil.printDate"/>
    </jxb:bindings>
  </jxb:bindings>
</jxb:bindings>
