	</message>	
	<!-- Tahvel 2 NEW ELEMENTS END -->
	<message name="registerPracticeContractResponse">
		<part name="return" type="tns:answer"/>
	</message>
	<!-- Tahvel 2 NEW ELEMENTS START -->
	<message name="cancelPracticeContractResponse">
		<part name="return" type="tns:answer"/>
