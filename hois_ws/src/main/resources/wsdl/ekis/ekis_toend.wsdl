	</message>
	<message name="registerDirectiveResponse">
		<part name="return" type="tns:answer"/>
	</message>
	<message name="deleteDirectiveRequest">
		<part name="q_guid" type="xsd:string"/>
		<part name="ois_id" type="xsd:string"/>
		<part name="status" type="xsd:string"/>
