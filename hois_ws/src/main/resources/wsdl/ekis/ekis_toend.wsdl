	</message>
	<message name="registerCertificateResponse">
		<part name="return" type="tns:answer"/>
	</message>
	<message name="registerDirectiveRequest">
		<part name="q_guid" type="xsd:string"/>
		<part name="ehis_id" type="xsd:int"/>
		<part name="ois_id" type="xsd:string"/>
		<part name="directive_type" type="xsd:string"/>
