		<operation name="registerCertificate">
			<soap:operation soapAction="" style="rpc"/>
			<input>
				<soap:body use="literal" namespace="urn:ekis" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</input>
			<output>
