		</operation>
		<operation name="deleteDirective">
			<input message="tns:deleteDirectiveRequest"/>
			<output message="tns:deleteDirectiveResponse"/>
		</operation>
		<operation name="registerPracticeContract">
			<input message="tns:registerPracticeContractRequest"/>
			<output message="tns:registerPracticeContractResponse"/>
		</operation>
	</portType>
	<binding name="wdoisBinding" type="tns:wdoisPortType">
