					<xsd:element name="avg_grade" type="xsd:string"/>
				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="contentArray">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" name="content" type="tns:content"/>
				</xsd:sequence>
			</xsd:complexType>
		</xsd:schema>
	</types>
	<message name="registerCertificateRequest">
		<part name="q_guid" type="xsd:string"/>
		<part name="ehis_id" type="xsd:string"/>
